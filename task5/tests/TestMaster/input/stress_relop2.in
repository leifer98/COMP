int bigger(int x, int y) {
    if (x > y) {
        return 1;
    }
    return 0;
}

int smaller(int x, int y) {
    if (x < y) {
        return 1;
    }
    return 0;
}

int equal(int x, int y) {
    if (x == y) {
        return 1;
    }
    return 0;
}

