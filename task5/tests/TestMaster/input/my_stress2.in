// Stress Test 2: Deeply Nested Blocks and Loops
void main() {
    int sum = 0;
    int i = 0;
    while (i < 3) {
         int j = 0;
         while (j < 3) {
              int k = 0;
              while (k < 3) {
                  sum = sum + 1;
                  k = k + 1;
              }
              j = j + 1;
         }
         i = i + 1;
    }
    printi(sum); // Expected output: 27
}
