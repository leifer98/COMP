void main() {
    int i = 0;
    while (i < 10) {
        print("Outer loop iteration: ");
        printi(i);
        print("\n");

        int j = 1;

        while (j <= 3) {
            print("\tInner loop iteration: ");
            printi(j);
            print("\n");
            j = j + 1;
        }

        i = i + 1;
    }
}
